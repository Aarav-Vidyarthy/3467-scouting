{
  "$schema": "../schema.json",
  "title": "QRScout",
  "page_title": "Crescendo",
  "sections": [
    {
      "name": "Prematch",
      "preserveDataOnReset": true,
      "fields": [
        {
          "title": "Scouter Initials",
          "type": "text",
          "required": true,
          "code": "scouter"
        },
        {
          "title": "Match Number",
          "type": "number",
          "required": true,
          "code": "matchNumber"
        },
        {
          "title": "Robot",
          "type": "select",
          "required": true,
          "code": "robot",
          "choices": {
            "R1": "Red 1",
            "R2": "Red 2",
            "R3": "Red 3",
            "B1": "Blue 1",
            "B2": "Blue 2",
            "B3": "Blue 3"
          },
          "defaultValue": "R1"
        },
        {
          "title": "Team Number",
          "type": "number",
          "required": true,
          "code": "teamNumber"
        },
        {
          "title": "Human Player at Amp",
          "type": "boolean",
          "required": false,
          "defaultValue": false,
          "code": "humanPlayer"
        },
        {
          "title": "No Show",
          "type": "boolean",
          "defaultValue": false,
          "required": false,
          "code": "noShow"
        },
        {
          "title": "Starting Side",
          "type": "select",
          "required": false,
          "choices": {
            "Ce": "Center",
            "AS": "Amp Side",
            "SS": "Source Side"
          },
          "defaultValue": "Center"
        }
      ]
    },
    {
      "name": "Autonomous",
      "fields": [
        {
          "code": "aum",
          "title": "Exited starting zone(black line)",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "title": "auto notes order(DONT DO):",
          "type": "select",
          "choices": {
            "AP": "P (preloaded note),",
            "AB1": "B1 (amp side wing note, blue alliance),",
            "AB2": "B2 (aligned with speaker, blue alliance),",
            "AB3": "B3 (podium note, blue alliance),",
            "AR1": "R1 (amp side wing note, red alliance),",
            "AR2": "R2 (aligned with speaker, red alliance),",
            "AR3": "R3 (podium note, red alliance),",
            "M1": "M1 (midline note closest to amp),",
            "M2": "M2 (midline note 2nd from amp),",
            "M3": "M3 (centermost note on midline),",
            "M4": "M4 (midline note 2nd from source),",
            "M5": "M5 (midline note closest to amp),",
            "NN": "NN (No Notes scored)"
          }
        },
        {
          "title": "Auto Notes Scored, no spaces, etc b1m3m4m5",
          "type": "text",
          "code": "scouter"
        },
        {
          "code": "Auto Fou/Tech",
          "title": " Auto Foul",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        }
      ]
    },
    {
      "name": "Teleop",
      "fields": [
        {
          "code": "tamps",
          "title": "Tele Amp Scored",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "tampm",
          "title": "Tele Amp Missed",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "tsc",
          "title": "Tele Speaker Scored",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "tsm",
          "title": "Tele Speaker Missed",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "Tele Fou/Tech",
          "title": " Tele Foul",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "title": "Note Pickup Location",
          "type": "select",
          "required": true,
          "code": "Npl",
          "choices": {
            "N": "None",
            "G": "Ground",
            "MS": "Human Player",
            "B": "Both"
          },
          "defaultValue": "None"
        }
      ]
    },
    {
      "name": "Endgame",
      "fields": [
        {
          "title": "End Position",
          "type": "select",
          "required": true,
          "code": "epo",
          "choices": {
            "No": "No Climb",
            "P": "Parked",
            "Mc": "Middle Chain",
            "Sc": "Side Chain"
          },
          "defaultValue": "No Climb"
        },
        {
          "code": "nit",
          "title": "Note in trap",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "title": "Harmony",
          "type": "select",
          "required": true,
          "code": "harm",
          "choices": {
            "Not": "Not Completed",
            "Two": "Achieved (2 robots, climbed first)",
            "Thr": "Achieved (2 robots, climbed second)",
            "Four": "Achieved (3 robots)"
          },
          "defaultValue": "Not Completed"
        }
      ]
    },
    {
      "name": "Postmatch",
      "fields": [
        {
          "code": "dr",
          "title": "Defense Skill",
          "type": "select",
          "choices": {
            "1": "Not Effective",
            "2": "Average",
            "3": "Very Effective",
            "x": "Not Observed"
          },
          "defaultValue": "x",
          "required": false
        },
        {
          "code": "d",
          "title": "Died",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "to",
          "title": "Tipped Over",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "yc",
          "title": "Yellow Card",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "rc",
          "title": "Red Card",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "co",
          "title": "Comments",
          "type": "text",
          "min": 0,
          "max": 50,
          "required": false
        }
      ]
    }
  ]
}
