{
  "$schema": "../schema.json",
  "title": "QRScout",
  "page_title": "Crescendo",
  "sections": [
    {
      "name": "Prematch",
      "preserveDataOnReset": true, 
      "fields": [
        {
          "title": "Scouter Initials",
          "type": "text",
          "required": true,
          "code": "scouter"
        },
        {
          "title": "Match Number",
          "type": "number",
          "required": true,
          "code": "matchNumber"
        },
        {
          "title": "Robot",
          "type": "select",
          "required": true,
          "code": "robot",
          "choices": {
            "R1": "Red 1",
            "R2": "Red 2",
            "R3": "Red 3",
            "B1": "Blue 1",
            "B2": "Blue 2",
            "B3": "Blue 3"
          },
          "defaultValue": "R1"
        },
        {
          "title": "Team Number",
          "type": "number",
          "required": true,
          "code": "teamNumber"
        },
        {
          "title": "No Show",
          "type": "boolean",
          "defaultValue": false,
          "required": false,
          "code": "noShow"
        },
        {
          "title": "Starting Side",
          "type": "select",
          "required": false,
          "choices": {
            "SoS": "Source Side",
            "SsS": "Source Side Subwoofer",
            "MiS": "Middle Subwoofer",
            "AsS": "Amp Side Subwoofer",
            "AmS": "Amp Side"
          },
          "defaultValue": "Middle Subwoofer"
        }
      ]
    },
    {
      "name": "Autonomous",
      "fields": [
        {
          "code": "aum",
          "title": "Exited starting zone (black line)",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "a_gp_Path",
          "title": "Auto Game Piece Path",
          "type": "select",
          "multiSelect": true,
          "choices": {
            "1": "Speaker 3 (Source Side)",
            "2": "Speaker 2 (Middle)",
            "3": "Speaker 1 (Amp Side)",
            "4": "Midline 5 (Source Edge)",
            "5": "Midline 4",
            "6": "Midline 3 (Middle)",
            "7": "Midline 2",
            "8": "Midline 1 (Amp Edge)"
          }
        },
        {
          "title": "Auto Notes Scored",
          "type": "counter",
          "code": "autoscore",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "Auto Fou/Tech",
          "title": " Auto Foul",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        }
      ]
    },
    {
      "name": "Teleop",
      "fields": [
        {
          "code": "tamps",
          "title": "Tele Amp Scored",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "tampm",
          "title": "Tele Amp Missed",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "tsc",
          "title": "Tele Speaker Scored",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "tsm",
          "title": "Tele Speaker Missed",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "tfed",
          "title": "Tele Notes Fed (Shot back for other alliance members to score)",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "Tele Fou/Tech",
          "title": " Tele Foul",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "title": "Note Pickup Location",
          "type": "select",
          "required": true,
          "code": "Npl",
          "choices": {
            "N": "None",
            "G": "Ground",
            "MS": "Human Player",
            "B": "Both"
          },
          "defaultValue": "None"
        }
      ]
    },
    {
      "name": "Endgame",
      "fields": [
        {
          "title": "End Position",
          "type": "select",
          "required": true,
          "code": "epo",
          "choices": {
            "No": "No Climb",
            "P": "Parked",
            "HC": "Hanging on chain"
          },
          "defaultValue": "No Climb"
        },
        {
          "code": "nit",
          "title": "Note in trap",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "title": "Harmony",
          "type": "select",
          "required": true,
          "code": "harm",
          "choices": {
            "First": "Not Completed",
            "Second": "Achieved (2 robots)",
            "Third": "Achieved (3 robots)"
          },
          "defaultValue": "Not Completed"
        }
      ]
    },
    {
      "name": "Postmatch",
      "fields": [
        {
          "code": "ns",
          "title": "Note Stuck",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "dr",
          "title": "Defense Skill",
          "type": "select",
          "choices": {
            "1": "Not Effective",
            "2": "Average",
            "3": "Very Effective",
            "x": "Not Observed"
          },
          "defaultValue": "x",
          "required": false
        },
        {
          "code": "d",
          "title": "Died",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "to",
          "title": "Tipped Over",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "yc",
          "title": "Yellow Card",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "rc",
          "title": "Red Card",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "co",
          "title": "Comments",
          "type": "text",
          "min": 0,
          "max": 280,
          "required": false
        }
      ]
    }
  ]
}
